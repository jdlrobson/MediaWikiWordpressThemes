{
	"name": "Neve",
	"version": "1.0.0",
	"author": [
		"[https://skins.wmflabs.org skins.wmflabs.org v.2.0]",
		"[https://github.com/jdlrobson/wordpress-theme-to-mediawiki-skin wordpress theme to mediawiki skin]"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:Neve",
	"descriptionmsg": "neve-skin-desc",
	"namemsg": "skinname-neve",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.38.0"
	},
	"ValidSkinNames": {
		"neve": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "neve",
					"responsive": true,
					"toc": false,
					"messages": [
						"neve-no-categories",
						"sitetitle",
						"search",
						"otherlanguages",
						"tagline",
						"neve-46",
						"neve-27",
						"neve-51",
						"neve-authors",
						"neve-52"
					],
					"styles": [
						"mediawiki.ui.icon",
						"mediawiki.ui.button",
						"skins.neve.styles"
					],
					"scripts": [
						"skins.neve"
					],
					"authors": [
						"[https://skins.wmflabs.org skins.wmflabs.org v.2.0]",
						"[https://github.com/jdlrobson/wordpress-theme-to-mediawiki-skin wordpress theme to mediawiki skin]"
					],
					"bodyClasses": [
						" nv-blog-grid",
						"nv-sidebar-full-width",
						"menu_sidebar_slide_left",
						"single-post"
					],
					"version": "trunk"
				}
			]
		}
	},
	"MessagesDirs": {
		"Neve": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.neve.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": false,
				"elements": false,
				"content-tables": true,
				"content-links": false,
				"content-media": true,
				"content-links-external": false,
				"interface-message-box": true,
				"interface-category": false,
				"toc": true
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": [
				"resources/common.css",
				"resources/skin.less"
			]
		},
		"skins.neve": {
			"targets": [
				"desktop",
				"mobile"
			],
			"packageFiles": [
				"resources/skin.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Neve"
	},
	"ResourceModuleSkinStyles": {},
	"manifest_version": 2
}
